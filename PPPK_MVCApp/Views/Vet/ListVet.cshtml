@model IEnumerable<PPPK_MVCApp.Veterinarian>

@{
    ViewBag.Title = "ListVet";
}

<h2>ListVet</h2>

<p>@Html.ActionLink("HOME", "Index", "Home", null, new { @class = "btn btn-primary btn-lg" })</p>

<p>
    @Html.ActionLink("Create New", "CreateVet")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IDVeterinarian)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IDVeterinarian)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditVet", new { id=item.IDVeterinarian }) |
            @Html.ActionLink("Details", "DetailsVet", new { id = item.IDVeterinarian }) |
            @Html.ActionLink("Delete", "DeleteVet", new { id = item.IDVeterinarian })
        </td>
    </tr>
}

</table>
